<body class="no-skin">
<%- include('public/nav')%>

<div class="main-container" id="main-container">
	<%- include('public/side') %>
	<div class="main-content">
		<div id="content" class="main-content-inner">
		<div class="page-content">
			<div class="row">
			<div class="col-xs-12">
				<div class="widget-box">
					<div class="widget-header">
						<h4 class="widget-title">文档管理</h4>						
					</div>
					<div class="widget-body">
						<div class="widget-main">
							<div class="row">
								<div class="col-xs-12 col-sm-12">
									<label class="col-sm-1 control-label no-padding lm-5" for="ids">版本：</label>
									<div class="col-sm-10">
										<select class="select" name="type">   
								        	<option value="0">中文</option>
								        	<option value="1">英文</option>
								     	 </select>
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-xs-12 col-sm-12">
									<label class="col-sm-1 control-label no-padding lm-5" for="ids">文件：</label>
									<div class="col-sm-10">
										<input type="text" />
										<a class="btn btn-primary j-btn-upload fileinput-button" id="uploadFile">上传<input type="file" name="file" tabindex="-1"></a>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
			</div><!-- end search box row -->

			<div class="row">
				<div id="grid_list" class="col-xs-12">
				</div>
			</div><!-- end grid row-->
		</div>
		</div>
	</div>
</div>
<div id="mask" class="widget-box-overlay hide"></div>
<script id="delTpl" type="text/html">
	<div class="row">
		文件名：TCL通讯支付SDK接入文档
	</div>
	<div class="row">
		 删除后，此文件将不可恢复！且商户服务系统不可显示与查看。
	</div>
</script>
<%- include('public/footscript1') %>
<%- include('public/footscript2') %>

